{
	"name": "nickdekruijk/leap",
	"keywords": ["leap", "laravel", "laravel easy admin panel"],
	"description": "Laravel Easy Admin Panel",
	"license": "MIT",
	"authors": [{
		"name": "Nick de Kruijk",
		"email": "git@nickdekruijk.nl"
	}],
	"require": {
		"php": ">=8.2",
		"blade-ui-kit/blade-icons": "^1.5",
		"danharrin/livewire-rate-limiting": "^2.0",
		"intervention/image-laravel": "^1.3",
		"laravel/framework": "^11.0|^12.0",
		"livewire/livewire": "^3.7",
		"owenvoke/blade-fontawesome": "^2.3",
		"nickdekruijk/minify": "^3.0",
		"spatie/laravel-translatable": "^6.10"
	},
	"autoload": {
		"psr-4": {
			"NickDeKruijk\\Leap\\": "src/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"NickDeKruijk\\Leap\\ServiceProvider"
			],
			"aliases": {
				"Leap": "NickDeKruijk\\Leap\\Facade"
			}
		}
	}
}