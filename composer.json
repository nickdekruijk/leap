{
	"name": "nickdekruijk/leap",
	"keywords": ["leap", "laravel", "laravel easy admin panel"],
	"description": "Laravel Easy Admin Panel",
	"license": "MIT",
	"authors": [{
		"name": "Nick de Kruijk",
		"email": "git@nickdekruijk.nl"
	}],
	"require": {
		"php": ">=8.1",
		"blade-ui-kit/blade-icons": "^1.5",
		"laravel/framework": "^10.0",
		"livewire/livewire": "^3.0",
		"owenvoke/blade-fontawesome": "^2.3",
		"scssphp/scssphp": "^1.11"
	},
	"autoload": {
		"psr-4": {
			"NickDeKruijk\\Leap\\": "src/"
		}
	},
	"minimum-stability": "beta",
	"extra": {
		"laravel": {
			"providers": [
				"NickDeKruijk\\Leap\\ServiceProvider"
			],
			"aliases": {
				"Leap": "NickDeKruijk\\Leap\\Facade"
			}
		}
	}
}